     ( verticalPitch              "METAL5"   0.41 )
     ( horizontalPitch            "METAL5"   0.41 )
     ( horizontalOffset           "METAL5"   0.205 )
     ( verticalOffset             "METAL5"   0.205 )

     ( verticalPitch              "METAL6"   0.46 )
     ( horizontalPitch            "METAL6"   0.46 )
