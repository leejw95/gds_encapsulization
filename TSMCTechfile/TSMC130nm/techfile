constraintGroups(

 ;( group	[override] )
 ;( -----	---------- )
  ( "virtuosoDefaultSetup"	nil

    interconnect(
     ( validLayers   (METAL8  METAL7  METAL6  METAL5  METAL4  METAL3  METAL2  METAL1  POLYG  ) )
     ( validVias     (M8_M7  M7_M6  M6_M5  M5_M4  M4_M3  M3_M2  M2_M1  M1_PO  ) )
    ) ;interconnect

  ) ;virtuosoDefaultSetup

 ;( group	[override] )
 ;( -----	---------- )
  ( "virtuosoDefaultExtractorSetup"	nil

    interconnect(
     ( validLayers   (PDKREC  METAL8  VIA78  METAL7  VIA67  METAL6  VIA56  METAL5  VIA45  METAL4  VIA34  METAL3  VIA23  METAL2  VIA12  METAL1  CONT  POLYG  OD  NWELL  ) )
     ( validVias     (M8_M7  M7_M6  M6_M5  M5_M4  M4_M3  M3_M2  M2_M1  M1_SUB  M1_PACTIVE  M1_NWELL  M1_NACTIVE  M1_NPOLYG  M1_PPOLYG  M1_PO  M1_OD  ) )
    ) ;interconnect
  ) ;virtuosoDefaultExtractorSetup

 ;( group	[override] )
