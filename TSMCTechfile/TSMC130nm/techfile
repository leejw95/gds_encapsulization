     ( validLayers   (METAL1  METAL2  METAL3  METAL4  METAL5  METAL6  METAL7  METAL8  ) )
     ( validVias     (M2_M1  M3_M2  M4_M3  M5_M4  M6_M5  M7_M6  M8_M7  ) )
    ) ;interconnect

    routingGrids(
     ( verticalPitch              "METAL1"   0.41 )
