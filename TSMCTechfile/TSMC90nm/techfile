     ( maxDensity                "M6"	
	(( "step"	 nil 	 nil) )
	(
	   10.0      90.0      
	   50.0      70.0      
	)
     )
     ( minSpacing                "M7"	
	(( "width"   nil  nil 	 "length"   nil   nil  )	 )
	(
	   (0.0       0.0       )	0.14      
	   (0.0       0.52      )	0.14      
	   (0.0       1.5       )	0.14      
	   (0.0       4.5       )	0.14      
	   (0.21      0.0       )	0.14      
	   (0.21      0.52      )	0.19      
	   (0.21      1.5       )	0.19      
	   (0.21      4.5       )	0.19      
	   (1.5       0.0       )	0.14      
	   (1.5       0.52      )	0.19      
	   (1.5       1.5       )	0.5       
	   (1.5       4.5       )	0.5       
	   (4.5       0.0       )	0.14      
	   (4.5       0.52      )	0.19      
