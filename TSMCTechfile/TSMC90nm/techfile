    ) ;spacingTables

    orderedSpacings(
     ( minEnclosure               "NW"	"OD"		0.17 )
     ( minEnclosure               "NT_N"	"OD"		0.26 )
     ( minEnclosure               "NW"	"OD_18"		0.62 )
     ( minEnclosure               "NW"	"OD_25"		0.62 )
     ( minEnclosure               "NW"	"OD_33"		0.62 )
     ( minEnclosure               "OD_18"	"NW"		0.62 )
     ( minEnclosure               "OD_25"	"NW"		0.62 )
     ( minEnclosure               "OD_33"	"NW"		0.62 )
     ( minEnclosure               "OD_18"	"OD"		0.27 )
     ( minEnclosure               "OD_25"	"OD"		0.27 )
     ( minEnclosure               "OD_33"	"OD"		0.27 )
     ( minEnclosure               "PO"	"OD"		0.16 )
     ( minEnclosure               "OD"	"PO"		0.15 )
     ( minEnclosure               "PP"	"OD"		0.02 )
     ( minEnclosure               "NP"	"OD"		0.02 )
     ( minEnclosure               "RPO"	"OD"		0.22 )
     ( minEnclosure               "RPO"	"PO"		0.22 )
     ( minEnclosure               "OD"	"RPO"		0.22 )
     ( minEnclosure               "OD"	"CO"		0.04 )
     ( minEnclosure               "M9"	"VIAD"		0.08 )
     ( minEnclosure               "MD"	"VIAD"		0.08 )
     ( minOppExtension            "PO"	"CO"		(0.02 0.05) )
     ( minOppExtension            "M1"	"CO"		(0.0 0.05) )
     ( minOppExtension            "M1"	"VIA1"		(0.005 0.05) )
     ( minOppExtension            "M2"	"VIA1"		(0.005 0.05) )
     ( minOppExtension            "M2"	"VIA2"		(0.005 0.05) )
     ( minOppExtension            "M3"	"VIA2"		(0.005 0.05) )
     ( minOppExtension            "M3"	"VIA3"		(0.005 0.05) )
     ( minOppExtension            "M4"	"VIA3"		(0.005 0.05) )
     ( minOppExtension            "M4"	"VIA4"		(0.005 0.05) )
