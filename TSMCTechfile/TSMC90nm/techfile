multipartPathTemplates(
; ( name [masterPath] [offsetSubpaths] [encSubPaths] [subRects] )
; 
;   masterPath:
;   (layer [width] [choppable] [endType] [beginExt] [endExt] [justify] [offset]
;   [connectivity])
; 
;   offsetSubpaths:
;   (layer [width] [choppable] [separation] [justification] [begOffset] [endOffset]
;   [connectivity])
; 
;   encSubPaths:
;   (layer [enclosure] [choppable] [separation] [begOffset] [endOffset]
;   [connectivity])
; 
;   subRects:
;   (layer [width] [length] [choppable] [separation] [justification] [space] [begOffset] [endOffset] [gap] 
;   [connectivity] [beginSegOffset] [endSegOffset])
; 
;   connectivity:
;   ([I/O type] [pin] [accDir] [dispPinName] [height] [ layer]
;    [layer] [justification] [font] [textOptions] [orientation]
;    [refHandle] [offset])
; 
; ( --------------------------------------------------------------------- )
  (NWellGuardring 
    (("OD" "drawing")	0.2	t	truncate  	0.0)
    ((("M1" "drawing")	0.22	t	0.0	center    	0         	0         )
     (("NP" "drawing")	0.24	t	0         	center    	0         	0         )
    )
    nil
    ((("CO" "drawing")	0.12	0.12	t	0.0	center    	nil	-0.05	-0.05	distribute	nil	0.0	0.0)
    )
  )
  (PSubGuardring 
    (("OD" "drawing")	0.2	t	truncate  	0.0)
    ((("M1" "drawing")	0.22	t	0.0	center    	0         	0         )
     (("PP" "drawing")	0.24	t	0         	center    	0         	0         )
    )
    nil
    ((("CO" "drawing")	0.12	0.12	t	0.0	center    	nil	-0.05	-0.05	distribute	nil	0.0	0.0)
    )
  )
)  ;multipartPathTemplates


;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;
; Opus Symbolic Device Class Definition
;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

tcCreateDeviceClass( "symbolic" "syEnhContact"
    ; class parameters
    ( (viaLayer "Cont") (viaLayer2 "") (encByVia 0.0) 
      (layer1 "Diff") (layer1Implant "") (layer1ImpEnc 0.0) 
      (layer2 "Metal1") (layer2Implant "") (layer2ImpEnc 0.0) 
      (encByLayer1 0.0) (encByLayer2 0.0) (layer1XEnc 0.7) 
      (layer1YEnc 0.7) (layer2XEnc 0.7) (layer2YEnc 0.7) )
    ; formal parameters
    ( (w 1.4) (l 1.4) (row 1) 
      (column 1) (xPitch 3.3) (yPitch 3.3) 
      (offset 0.0) (side "center") (xBias "center") 
      (yBias "center") (layer1Dir "") (layer2Dir "") 
      (leakLength 0.0) (areaPin "") (intersectType "normal") )
    ; IL codes specifying geometry
    
    (setq layer1X 
	(case layer1Dir 
	    (("x" "xy" "xt" "xb") layer1XEnc) 
	    (t encByLayer1)
	)
    )
    (setq layer1Y 
	(case layer1Dir 
	    (("y" "yx" "yl" "yr") layer1YEnc) 
	    (t encByLayer1)
	)
    )
    (setq layer2X 
	(case layer2Dir 
	    (("x" "xy" "xt" "xb") layer2XEnc) 
	    (t encByLayer2)
	)
    )
    (setq layer2Y 
	(case layer2Dir 
	    (("y" "yx" "yl" "yr") layer2YEnc) 
	    (t encByLayer2)
	)
    )
    (setq direction1 
	(case layer1Dir 
	    ("x" 
		(list "left" "right")
	    ) 
	    ("xt" 
		(list "left" "right" "top")
	    ) 
	    ("xb" 
		(list "left" "right" "bottom")
	    )
	    ("y" 
		(list "top" "bottom")
	    ) 
	    ("yl" 
		(list "top" "bottom" "left")
	    ) 
	    ("yr" 
		(list "top" "bottom" "right")
	    ) 
	    (t 
		(list "left" "right" "top" "bottom")
	    )
	)
    )
    (setq direction2 
	(case layer2Dir 
	    ("x" 
		(list "left" "right")
	    ) 
	    ("xt" 
		(list "left" "right" "top")
	    ) 
	    ("xb" 
		(list "left" "right" "bottom")
	    )
	    ("y" 
		(list "top" "bottom")
	    ) 
	    ("yl" 
		(list "top" "bottom" "left")
	    ) 
	    ("yr" 
		(list "top" "bottom" "right")
	    ) 
	    (t 
		(list "left" "right" "top" "bottom")
	    )
	)
    )
    (case side 
	(("left-leak" "right-leak") 
	    (setq orientation "vertical") 
	    (setq layer1X encByLayer1) 
	    (setq layer2X encByLayer2)
	) 
	(("top-leak" "bottom-leak") 
	    (setq orientation "horizontal") 
	    (setq layer1X layer1Y) 
	    (setq layer2X layer2Y) 
	    (setq layer1Y encByLayer1)
	    (setq layer2Y encByLayer2) 
	    (setq x xPitch) 
	    (setq xPitch yPitch) 
	    (setq yPitch x) 
	    (setq x w)
	    (setq w l) 
	    (setq l x)
	) 
	(t 
	    (setq orientation side)
	)
    )
    (if 
	(and 
	    (nequal intersectType "normal") 
	    (nequal intersectType "normal_full_farm")
	) then 
	(setq tfId 
	    (techGetTechFile 
		(ddGetObj 
		    (getSGq tcCellView libName)
		)
	    )
	) 
	(setq grid_res 
	    (techGetMfgGridResolution tfId)
	)
	(setq sqrt2 
	    (sqrt 2.0)
	) 
	(setq sqrt2i 
	    (quotient 1 sqrt2)
	) 
	(setq viaSize w) 
	(setq viaSpace 
	    (difference xPitch w)
	) 
	(setq viaSize45_org 
	    (times sqrt2 viaSize)
	)
	(setq viaSpace45_org 
	    (quotient 
		(round 
		    (times viaSpace sqrt2i 1000.0)
		) 1000.0
	    )
	) 
	(setq viaSize45 
	    (times 
		(round 
		    (quotient viaSize45_org grid_res)
		) grid_res
	    )
	) 
	(when 
	    (lessp viaSize45 viaSize45_org) 
	    (setq viaSize45 
		(plus viaSize45 grid_res)
	    )
	) 
	(setq viaSpace45 
	    (times 
		(round 
		    (quotient viaSpace45_org grid_res)
		) grid_res
	    )
	) 
	(when 
	    (lessp viaSpace45 viaSpace45_org) 
	    (setq viaSpace45 
		(plus viaSpace45 grid_res)
	    )
	)
	(setq viaStep 
	    (range xPitch 0.0)
	) 
	(setq viaStep2 
	    (range xPitch yPitch)
	) 
	(setq viaStep3 viaStep2) 
	(when 
	    (equal intersectType "diamond") 
	    (setq viaStep_org 
		(quotient 
		    (round 
			(times 
			    (plus viaSize45 viaSpace) sqrt2i 1000.0
			)
		    ) 1000.0
		)
	    ) 
	    (setq viaStep 
		(times 
		    (round 
			(quotient viaStep_org grid_res)
		    ) grid_res
		)
	    ) 
	    (when 
		(lessp viaStep viaStep_org) 
		(setq viaStep 
		    (plus viaStep grid_res)
		)
	    )
	    (setq viaStep 
		(range viaStep viaStep)
	    ) 
	    (setq viaStep2 
		(range 
		    (minus 
			(difference viaSpace viaSpace45)
		    ) 
		    (plus viaSize viaSpace)
		)
	    ) 
	    (setq viaStep3 
		(range 
		    (minus 
			(plus viaSize viaSpace)
		    ) 
		    (difference viaSpace viaSpace45)
		)
	    ) 
	    (setq viaStepOddCol 
		(range 
		    (minus 
			(difference viaSpace viaSpace45)
		    ) 
		    (plus viaSize viaSpace)
		)
	    ) 
	    (setq viaStepEvenCol 
		(range 
		    (plus viaSize viaSpace) 
		    (minus 
			(difference viaSpace viaSpace45)
		    )
		)
	    )
	) 
	(setq x1 
	    (setq y1 0.0)
	)
	(setq x2 
	    (plus x1 viaSize)
	) 
	(setq y2 
	    (plus y1 viaSize)
	) 
	(setq netId 
	    (dbMakeNet tcCellView "pdd")
	) 
	(if 
	    (and 
		(greaterp column 0) 
		(greaterp row 0)
	    ) then 
	    (setq firstVia 
		(ncons 
		    (dbCreateRect tcCellView viaLayer 
			(list 
			    (range x1 y1) 
			    (range x2 y2)
			)
		    )
		)
	    ) else
	    (when 
		(equal row 0) 
		(setq row 1)
	    ) 
	    (when 
		(equal column 0) 
		(setq column 1)
	    ) 
	    (setq firstVia 
		(ncons 
		    (dbCreateRect tcCellView viaLayer 
			(list 
			    (range x1 y1) 
			    (range x2 y2)
			)
		    )
		)
	    ) 
	    (setq lay1Pts 
		(list 
		    (range 
			(difference x1 layer1X) 
			(difference y1 layer1Y)
		    ) 
		    (range 
			(plus x2 layer1X) 
			(plus y2 layer1Y)
		    )
		)
	    ) 
	    (setq lay2Pts 
		(list 
		    (range 
			(difference x1 layer2X) 
			(difference y1 layer2Y)
		    ) 
		    (range 
			(plus x2 layer2X) 
			(plus y2 layer2Y)
		    )
		)
	    )
	    (setq firstVia 
		(append1 firstVia 
		    (dbCreateRect tcCellView layer1 lay1Pts)
		)
	    ) 
	    (setq firstVia 
		(append1 firstVia 
		    (dbCreateRect tcCellView layer2 lay2Pts)
		)
	    )
	) 
	(setq dbId 
	    (dbCreateDot tcCellView viaLayer 
		(range 
		    (plus x1 
			(quotient viaSize 2.0)
		    ) 
		    (plus y1 
			(quotient viaSize 2.0)
		    )
		)
	    )
	)
	(setq dbId 
	    (dbCreatePin netId dbId)
	) 
	(setSGq dbId 
	    (list "top" "bottom" "left" "right") accessDir
	) 
	(setq x nil) 
	(setq viaStepRow 
	    (range 
		(times 
		    (round 
			(quotient 
			    (plus 
				(xCoord viaStep2) 
				(xCoord viaStep3)
			    ) grid_res
			)
		    ) grid_res
		) 
		(times 
		    (round 
			(quotient 
			    (plus 
				(yCoord viaStep2) 
				(yCoord viaStep3)
			    ) grid_res
			)
		    ) grid_res
		)
	    )
	) 
	(setq viaStepRowMin 
	    (range 
		(minus 
		    (car viaStep)
		) 
		(cadr viaStep)
	    )
	)
	(if 
	    (equal intersectType "diamond") then 
	    (for i 1 column 
		(setq nextVia firstVia)
		(if 
		    (lessp column 2) then 
		    (for j 2 row 
			(setq nextVia 
			    (foreach mapcar x nextVia 
				(dbCopyFig x tcCellView 
				    (list viaStepRowMin "R0")
				)
			    )
			)
		    ) else
		    (if 
			(oddp i) then 
			(for j 2 row 
			    (when 
				(oddp j) 
				(setq nextVia 
				    (foreach mapcar x nextVia 
					(dbCopyFig x tcCellView 
					    (list viaStepRow "R0")
					)
				    )
				)
			    )
			) else
			(for j 2 
			    (difference row 2) 
			    (when 
				(evenp j) 
				(setq nextVia 
				    (foreach mapcar x nextVia 
					(dbCopyFig x tcCellView 
					    (list viaStepRow "R0")
					)
				    )
				)
			    )
			)
		    )
		) 
		(if 
		    (greaterp column i) then 
		    (setq nextVia firstVia) 
		    (if 
			(lessp row 2) then 
			(setq firstVia 
			    (foreach mapcar x nextVia 
				(dbCopyFig x tcCellView 
				    (list viaStep "R0")
				)
			    )
			) else
			(if 
			    (oddp i) then 
			    (setq firstVia 
				(foreach mapcar x nextVia 
				    (dbCopyFig x tcCellView 
					(list viaStepOddCol "R0")
				    )
				)
			    ) else
			    (setq firstVia 
				(foreach mapcar x nextVia 
				    (dbCopyFig x tcCellView 
					(list viaStepEvenCol "R0")
				    )
				)
			    )
			)
		    )
		)
	    ) else
	    (for i 1 row 
		(setq nextVia firstVia)
		(for j 2 column 
		    (setq nextVia 
			(foreach mapcar x nextVia 
			    (dbCopyFig x tcCellView 
				(list viaStep "R0")
			    )
			)
		    )
		) 
		(unless 
		    (equal i row) 
		    (setq firstVia 
			(foreach mapcar x firstVia 
			    (dbCopyFig x tcCellView 
				(list viaStep2 "R0")
			    )
			)
		    )
		)
	    )
	) 
	(when 
	    (equal intersectType "diamond") 
	    (setq sqrt2 
		(sqrt 2.0)
	    ) 
	    (setq sqrt2_2 
		(times 0.5 sqrt2)
	    ) 
	    (setq coord nil)
	    (setq viaCuts 
		(dbGetTrueOverlaps tcCellView 
		    (getq tcCellView bBox) viaLayer
		)
	    ) 
	    (when viaCuts 
		(setq viaPoints nil) 
		(setq cutPoints nil) 
		(foreach cut viaCuts 
		    (setq point1 
			(car 
			    (getq cut bBox)
			)
		    ) 
		    (setq point3 
			(cadr 
			    (getq cut bBox)
			)
		    )
		    (setq point2 
			(range 
			    (xCoord point1) 
			    (yCoord point3)
			)
		    ) 
		    (setq point4 
			(range 
			    (xCoord point3) 
			    (yCoord point1)
			)
		    ) 
		    (setq cutPoints 
			(foreach mapcar coord 
			    (list point1 point2 point3 point4) 
			    (range 
				(difference 
				    (times sqrt2_2 
					(xCoord coord)
				    ) 
				    (times sqrt2_2 
					(yCoord coord)
				    )
				) 
				(plus 
				    (times sqrt2_2 
					(xCoord coord)
				    ) 
				    (times sqrt2_2 
					(yCoord coord)
				    )
				)
			    )
			)
		    ) 
		    (setq viaPoints 
			(append cutPoints viaPoints)
		    )
		)
		(setq minX 
		    (xCoord 
			(car viaPoints)
		    )
		) 
		(setq minY 
		    (yCoord 
			(car viaPoints)
		    )
		) 
		(setq maxX minX) 
		(setq maxY minY) 
		(foreach coord 
		    (cdr viaPoints) 
		    (setq xc 
			(xCoord coord)
		    ) 
		    (setq yc 
			(yCoord coord)
		    )
		    (when 
			(lessp xc minX) 
			(setq minX xc)
		    ) 
		    (when 
			(lessp yc minY) 
			(setq minY yc)
		    ) 
		    (when 
			(greaterp xc maxX) 
			(setq maxX xc)
		    ) 
		    (when 
			(greaterp yc maxY) 
			(setq maxY yc)
		    )
		)
		(setq dx1 
		    (ceiling 
			(quotient layer1X 
			    (times sqrt2_2 grid_res)
			)
		    )
		) 
		(setq dy1 
		    (ceiling 
			(quotient layer1Y 
			    (times sqrt2_2 grid_res)
			)
		    )
		) 
		(setq dx2 
		    (ceiling 
			(quotient layer2X 
			    (times sqrt2_2 grid_res)
			)
		    )
		) 
		(setq dy2 
		    (ceiling 
			(quotient layer2Y 
			    (times sqrt2_2 grid_res)
			)
		    )
		) 
		(setq min1X 
		    (difference minX 
			(times dx1 sqrt2_2 grid_res)
		    )
		)
		(setq min1Y 
		    (difference minY 
			(times dy1 sqrt2_2 grid_res)
		    )
		) 
		(setq max1X 
		    (plus maxX 
			(times dx1 sqrt2_2 grid_res)
		    )
		) 
		(setq max1Y 
		    (plus maxY 
			(times dy1 sqrt2_2 grid_res)
		    )
		) 
		(setq min2X 
		    (difference minX 
			(times dx2 sqrt2_2 grid_res)
		    )
		) 
		(setq min2Y 
		    (difference minY 
			(times dy2 sqrt2_2 grid_res)
		    )
		)
		(setq max2X 
		    (plus maxX 
			(times dx2 sqrt2_2 grid_res)
		    )
		) 
		(setq max2Y 
		    (plus maxY 
			(times dy2 sqrt2_2 grid_res)
		    )
		) 
		(setq boundsL1 
		    (foreach mapcar coord 
			(list 
			    (range min1X min1Y) 
			    (range min1X max1Y) 
			    (range max1X max1Y) 
			    (range max1X min1Y)
			) 
			(range 
			    (plus 
				(times sqrt2_2 
				    (xCoord coord)
				) 
				(times sqrt2_2 
				    (yCoord coord)
				)
			    ) 
			    (plus 
				(times 
				    (minus sqrt2_2) 
				    (xCoord coord)
				) 
				(times sqrt2_2 
				    (yCoord coord)
				)
			    )
			)
		    )
		) 
		(setq boundsL2 
		    (foreach mapcar coord 
			(list 
			    (range min2X min2Y) 
			    (range min2X max2Y) 
			    (range max2X max2Y) 
			    (range max2X min2Y)
			) 
			(range 
			    (plus 
				(times sqrt2_2 
				    (xCoord coord)
				) 
				(times sqrt2_2 
				    (yCoord coord)
				)
			    ) 
			    (plus 
				(times 
				    (minus sqrt2_2) 
				    (xCoord coord)
				) 
				(times sqrt2_2 
				    (yCoord coord)
				)
			    )
			)
		    )
		) 
		(setq boundsL1 
		    (list 
			(range 
			    (difference 
				(xCoord 
				    (car boundsL1)
				) 
				(times 0.5 grid_res)
			    ) 
			    (difference 
				(yCoord 
				    (car boundsL1)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (plus 
				(xCoord 
				    (nthelem 2 boundsL1)
				) 
				(times 0.5 grid_res)
			    ) 
			    (plus 
				(yCoord 
				    (nthelem 2 boundsL1)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (plus 
				(xCoord 
				    (nthelem 3 boundsL1)
				) 
				(times 0.5 grid_res)
			    ) 
			    (plus 
				(yCoord 
				    (nthelem 3 boundsL1)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (difference 
				(xCoord 
				    (nthelem 4 boundsL1)
				) 
				(times 0.5 grid_res)
			    ) 
			    (difference 
				(yCoord 
				    (nthelem 4 boundsL1)
				) 
				(times 0.5 grid_res)
			    )
			)
		    )
		)
		(setq boundsL2 
		    (list 
			(range 
			    (difference 
				(xCoord 
				    (car boundsL2)
				) 
				(times 0.5 grid_res)
			    ) 
			    (difference 
				(yCoord 
				    (car boundsL2)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (plus 
				(xCoord 
				    (nthelem 2 boundsL2)
				) 
				(times 0.5 grid_res)
			    ) 
			    (plus 
				(yCoord 
				    (nthelem 2 boundsL2)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (plus 
				(xCoord 
				    (nthelem 3 boundsL2)
				) 
				(times 0.5 grid_res)
			    ) 
			    (plus 
				(yCoord 
				    (nthelem 3 boundsL2)
				) 
				(times 0.5 grid_res)
			    )
			) 
			(range 
			    (difference 
				(xCoord 
				    (nthelem 4 boundsL2)
				) 
				(times 0.5 grid_res)
			    ) 
			    (difference 
				(yCoord 
				    (nthelem 4 boundsL2)
				) 
				(times 0.5 grid_res)
			    )
			)
		    )
		) 
		(dbCreatePolygon tcCellView layer1 boundsL1) 
		(dbCreatePolygon tcCellView layer2 boundsL2)
	    )
	) else 
	(setq cutPitchX xPitch) 
	(setq cutPitchY yPitch)
	(setq cutSizeX w) 
	(setq cutSizeY l) 
	(setq cutSpaceX 
	    (difference cutPitchX cutSizeX)
	) 
	(setq cutSpaceY 
	    (difference cutPitchY cutSizeY)
	) 
	(setq clusterSizeX 0.0)
	(setq clusterSizeY 0.0) 
	(setq boundarySizeX 0.0) 
	(setq boundarySizeY 0.0) 
	(setq numClustersX 0) 
	(setq numClustersY 0)
	(setq actualSizeX 0.0) 
	(setq acutalSizeY 0.0) 
	(setq numClusterCutsX 1) 
	(setq numClusterCutsY 1) 
	(setq clusterSpaceX 0.0)
	(setq clusterSpaceY 0.0) 
	(setq cutOptions nil) 
	(setq boundarySizeX 
	    (plus 
		(times cutPitchX 
		    (difference column 1.0)
		) cutSizeX
	    )
	) 
	(setq boundarySizeY 
	    (plus 
		(times cutPitchY 
		    (difference row 1.0)
		) cutSizeY
	    )
	) 
	(setq tfId 
	    (techGetTechFile tcCellView)
	)
	(setq viaArraySpacingTable 
	    (techGetSpacingRuleTable tfId "minLargeViaArraySpacing" viaLayer)
	) 
	(setq viaArrayThreshold 
	    (techGetSpacingRule tfId "minLargeViaArrayWidth" viaLayer)
	) 
	(when 
	    (and viaArraySpacingTable viaArrayThreshold) 
	    (setq viaArrayCutSpacing 
		(techGetSpacingRule tfId "minLargeViaArrayCutSpacing" viaLayer)
	    ) 
	    (when viaArrayCutSpacing 
		(setq cutSpaceX 
		    (max viaArrayCutSpacing cutSpaceX)
		) 
		(setq cutSpaceY 
		    (max viaArrayCutSpacing cutSpaceY)
		) 
		(setq cutPitchX 
		    (plus cutSpaceX cutSizeX)
		)
		(setq cutPitchY 
		    (plus cutSpaceY cutSizeY)
		)
	    ) 
	    (setq spacingRules 
		(cdr viaArraySpacingTable)
	    )
	    (setq clusterSpacingTable 
		(makeTable "" nil)
	    ) 
	    (setq i 1) 
	    (foreach x spacingRules 
		(when 
		    (oddp i) 
		    (setarray clusterSpacingTable x 
			(nthelem 
			    (plus i 1) spacingRules
			)
		    )
		) 
		(postincrement i)
	    ) 
	    (setq cutOptions 
		(sort 
		    (getq clusterSpacingTable ?) 
		    (quote lessp)
		)
	    ) 
	    (foreach option cutOptions 
		(when 
		    (or 
			(and 
			    (leqp 
				(difference 
				    (times option cutPitchX) cutSpaceX
				) boundarySizeX
			    ) 
			    (leqp 
				(difference 
				    (times option cutPitchY) cutSpaceY
				) boundarySizeY
			    )
			) 
			(equal intersectType "normal_full_farm")
		    ) 
		    (setq numClusterCutsX option) 
		    (setq numClusterCutsY option) 
		    (setq clusterSpaceX 
			(arrayref clusterSpacingTable option)
		    )
		    (setq clusterSpaceY 
			(arrayref clusterSpacingTable option)
		    )
		)
	    )
	) 
	(if 
	    (or 
		(and viaArrayThreshold 
		    (geqp boundarySizeX viaArrayThreshold) 
		    (geqp boundarySizeY viaArrayThreshold)
		) 
		(equal intersectType "normal_full_farm")
	    ) then 
	    (setq hasFarm t) else
	    (setq numClusterCutsX column) 
	    (setq numClusterCutsY row) 
	    (setq cutPitchX xPitch) 
	    (setq cutPitchY yPitch) 
	    (setq cutSpaceX 
		(difference cutPitchX cutSizeX)
	    )
	    (setq cutSpaceY 
		(difference cutPitchY cutSizeY)
	    ) 
	    (setq hasFarm nil)
	) 
	(setq clusterSizeX 
	    (difference 
		(times numClusterCutsX cutPitchX) cutSpaceX
	    )
	)
	(setq clusterSizeY 
	    (difference 
		(times numClusterCutsY cutPitchY) cutSpaceY
	    )
	) 
	(setq numClustersX 
	    (floor 
		(quotient boundarySizeX 
		    (plus clusterSizeX clusterSpaceX)
		)
	    )
	) 
	(when 
	    (lessp numClustersX 1) 
	    (setq numClustersX 1)
	) 
	(setq actualSizeX 
	    (times numClustersX 
		(plus clusterSizeX clusterSpaceX)
	    )
	) 
	(if 
	    (geqp 
		(difference boundarySizeX actualSizeX) clusterSizeX
	    ) then 
	    (postincrement numClustersX) 
	    (setq actualSizeX 
		(plus actualSizeX clusterSizeX)
	    )
	    else 
	    (setq actualSizeX 
		(difference actualSizeX clusterSpaceX)
	    )
	)
	(setq numClustersY 
	    (floor 
		(quotient boundarySizeY 
		    (plus clusterSizeY clusterSpaceY)
		)
	    )
	) 
	(when 
	    (lessp numClustersY 1) 
	    (setq numClustersY 1)
	) 
	(setq actualSizeY 
	    (times numClustersY 
		(plus clusterSizeY clusterSpaceY)
	    )
	) 
	(if 
	    (geqp 
		(difference boundarySizeY actualSizeY) clusterSizeY
	    ) then 
	    (postincrement numClustersY) 
	    (setq actualSizeY 
		(plus actualSizeY clusterSizeY)
	    )
	    else 
	    (setq actualSizeY 
		(difference actualSizeY clusterSpaceY)
	    )
	) 
	(setq rowTable 
	    (makeTable "" nil)
	)
	(setq columnTable 
	    (makeTable "" nil)
	) 
	(for i 1 numClustersX 
	    (setarray columnTable i numClusterCutsX)
	) 
	(for i 1 numClustersY 
	    (setarray rowTable i numClusterCutsY)
	) 
	(when 
	    (and hasFarm 
		(nequal intersectType "normal_full_farm")
	    ) 
	    (setq i 
		(difference numClusterCutsX 1)
	    ) 
	    (setq partialFound nil) 
	    (while 
		(and 
		    (geqp i 1) 
		    (null partialFound)
		) 
		(when 
		    (geqp 
			(plus 
			    (difference boundarySizeX actualSizeX clusterSpaceX 
				(times i cutPitchX)
			    ) cutSpaceX
			) 0.0
		    ) 
		    (postincrement numClustersX) 
		    (setarray columnTable numClustersX i) 
		    (setq actualSizeX 
			(difference 
			    (plus actualSizeX clusterSpaceX 
				(times i cutPitchX)
			    ) cutSpaceX
			)
		    )
		    (setq partialFound t)
		) 
		(postdecrement i)
	    )
	    (setq i 
		(difference numClusterCutsY 1)
	    ) 
	    (setq partialFound nil) 
	    (while 
		(and 
		    (geqp i 1) 
		    (null partialFound)
		) 
		(when 
		    (geqp 
			(plus 
			    (difference boundarySizeY actualSizeY clusterSpaceY 
				(times i cutPitchY)
			    ) cutSpaceY
			) 0.0
		    ) 
		    (postincrement numClustersY) 
		    (setarray rowTable numClustersY i) 
		    (setq actualSizeY 
			(difference 
			    (plus actualSizeY clusterSpaceY 
				(times i cutPitchY)
			    ) cutSpaceY
			)
		    )
		    (setq partialFound t)
		) 
		(postdecrement i)
	    )
	) 
	(setq xPitch cutPitchX)
	(setq yPitch cutPitchY) 
	(setq clusterPitchX 
	    (plus clusterSizeX clusterSpaceX)
	) 
	(setq clusterPitchY 
	    (plus clusterSizeY clusterSpaceY)
	) 
	(case orientation 
	    ("top" 
		(setq llY offset) 
		(setq urY 
		    (plus actualSizeY llY)
		) 
		(setq lleftY 
		    (minus urY)
		) 
		(setq urightY urY)
		(setq centerY offset) 
		(setq n 1)
	    ) 
	    ("bottom" 
		(setq urY 
		    (minus offset)
		) 
		(setq llY 
		    (difference urY actualSizeY)
		) 
		(setq urightY 
		    (minus llY)
		) 
		(setq lleftY llY)
		(setq centerY 
		    (minus offset)
		) 
		(setq n 1)
	    ) 
	    ("both" 
		(setq llY offset) 
		(setq urY 
		    (plus actualSizeY llY)
		) 
		(setq lleftY 
		    (minus urY)
		) 
		(setq urightY urY)
		(setq centerY offset) 
		(setq n 2)
	    )
	    ("vertical" 
		(setq offset 0.0) 
		(setq urightY 
		    (quotient actualSizeY 2.0)
		) 
		(setq urY urightY) 
		(setq lleftY 
		    (minus urightY)
		)
		(setq llY lleftY) 
		(setq centerY 0.0) 
		(setq row 1) 
		(setq column 1) 
		(setq n 1)
	    ) 
	    ("horizontal" 
		(setq offset 0.0) 
		(setq urightY 
		    (quotient actualSizeY 2.0)
		) 
		(setq urY urightY) 
		(setq lleftY 
		    (minus urightY)
		)
		(setq llY lleftY) 
		(setq centerY 0.0) 
		(setq row 1) 
		(setq column 1) 
		(setq n 1)
	    ) 
	    (t 
		(setq orientation "center") 
		(setq offset 0.0) 
		(setq urightY 
		    (quotient actualSizeY 2.0)
		) 
		(setq urY urightY)
		(setq lleftY 
		    (minus urightY)
		) 
		(setq llY lleftY) 
		(setq centerY 0.0) 
		(setq n 1)
	    )
	) 
	(setq urightX 
	    (quotient actualSizeX 2.0)
	)
	(setq lleftX 
	    (minus urightX)
	) 
	(setq moveX 
	    (case xBias 
		("right" 
		    (quotient 
			(difference cutSizeX actualSizeX) 2.0
		    )
		) 
		("left" 
		    (quotient 
			(difference actualSizeX cutSizeX) 2.0
		    )
		) 
		(t 0.0)
	    )
	) 
	(setq moveY 
	    (case yBias 
		("top" 
		    (quotient 
			(difference cutSizeY actualSizeY) 2.0
		    )
		) 
		("bottom" 
		    (quotient 
			(difference actualSizeY cutSizeY) 2.0
		    )
		) 
		(t 0.0)
	    )
	) 
	(setq net 
	    (dbMakeNet tcCellView "ppd")
	) 
	(setq x1 
	    (case side 
		("left-leak" 
		    (plus 
			(difference lleftX layer1X leakLength) encByLayer1
		    )
		) 
		("right-leak" 
		    (difference 
			(plus urightX layer1X) encByLayer1
		    )
		) 
		(t 
		    (difference lleftX layer1X)
		)
	    )
	)
	(setq x2 
	    (case side 
		("left-leak" 
		    (plus 
			(difference lleftX layer1X) encByLayer1
		    )
		) 
		("right-leak" 
		    (plus 
			(difference 
			    (plus urightX layer1X) encByLayer1
			) leakLength
		    )
		) 
		(t 
		    (plus urightX layer1X)
		)
	    )
	) 
	(setq layer1AreaPin 
	    (or 
		(equal areaPin "all") 
		(equal areaPin "layer1") 
		(equal areaPin "layer12")
	    )
	) 
	(setq layer2AreaPin 
	    (or 
		(equal areaPin "all") 
		(equal areaPin "layer2") 
		(equal areaPin "layer12")
	    )
	) 
	(setq viaAreaPin 
	    (or 
		(equal areaPin "via") 
		(equal areaPin "all")
	    )
	) 
	(setq via2AreaPin 
	    (or 
		(equal areaPin "via2") 
		(equal areaPin "all")
	    )
	)
	(when 
	    (lessp x1 x2) 
	    (setq y1 
		(case side 
		    ("bottom-leak" 
			(difference 
			    (plus 
				(difference lleftY layer1Y) encByLayer1
			    ) leakLength
			)
		    ) 
		    ("top-leak" 
			(difference 
			    (plus urightY layer1Y) encByLayer1
			)
		    ) 
		    (t 
			(difference lleftY layer1Y)
		    )
		)
	    ) 
	    (setq y2 
		(case side 
		    ("bottom-leak" 
			(plus 
			    (difference lleftY layer1Y) encByLayer1
			)
		    ) 
		    ("top-leak" 
			(plus 
			    (difference 
				(plus urightY layer1Y) encByLayer1
			    ) leakLength
			)
		    ) 
		    (t 
			(plus urightY layer1Y)
		    )
		)
	    ) 
	    (when 
		(lessp y1 y2) 
		(setq rect 
		    (dbCreateRect tcCellView layer1 
			(list 
			    (range x1 y1) 
			    (range x2 y2)
			)
		    )
		) 
		(dbMoveFig rect tcCellView 
		    (list 
			(range moveX moveY) "R0"
		    )
		) 
		(dbAddFigToNet rect net)
		(when layer1AreaPin 
		    (setq pin 
			(dbCreatePin net rect)
		    ) 
		    (dbSetq pin direction1 accessDir)
		) 
		(unless 
		    (equal layer1Implant "") 
		    (setq x1 
			(difference x1 layer1ImpEnc)
		    ) 
		    (setq x2 
			(plus x2 layer1ImpEnc)
		    ) 
		    (when 
			(lessp x1 x2) 
			(setq y1 
			    (difference y1 layer1ImpEnc)
			) 
			(setq y2 
			    (plus y2 layer1ImpEnc)
			) 
			(setq rect 
			    (dbCreateRect tcCellView layer1Implant 
				(list 
				    (range x1 y1) 
				    (range x2 y2)
				)
			    )
			)
			(dbMoveFig rect tcCellView 
			    (list 
				(range moveX moveY) "R0"
			    )
			) 
			(dbAddFigToNet rect net)
		    )
		)
	    )
	) 
	(unless layer1AreaPin 
	    (setq dot 
		(dbCreateDot tcCellView layer1 
		    (range 0.0 0.0)
		)
	    ) 
	    (setq pin 
		(dbCreatePin net dot)
	    ) 
	    (dbSetq pin direction1 accessDir)
	) 
	(unless via2AreaPin 
	    (when 
		(and 
		    (nequal viaLayer viaLayer2) 
		    (nequal viaLayer2 "")
		) 
		(setq dot 
		    (dbCreateDot tcCellView viaLayer2 
			(range 0.0 0.0)
		    )
		) 
		(setq pin 
		    (dbCreatePin net dot)
		) 
		(dbSetq pin 
		    (list "right" "left" "top" "bottom") accessDir
		)
	    )
	) 
	(setq originX lleftX) 
	(setq originY llY)
	(for i 1 n 
	    (for clusterCol 1 numClustersX 
		(setq lleftX 
		    (plus originX 
			(times 
			    (difference clusterCol 1.0) clusterPitchX
			)
		    )
		)
		(for clusterRow 1 numClustersY 
		    (setq llY 
			(plus originY 
			    (times 
				(difference clusterRow 1.0) clusterPitchY
			    )
			)
		    )
		    (setq numClusterCutsX 
			(arrayref columnTable clusterCol)
		    ) 
		    (setq numClusterCutsY 
			(arrayref rowTable clusterRow)
		    ) 
		    (for c 1 numClusterCutsX 
			(if 
			    (equal side "horizontal") then 
			    (setq lX 
				(difference 
				    (plus lleftX 
					(times 
					    (difference c 1.0) cutPitchX
					)
				    ) layer1X
				)
			    ) 
			    (setq uX 
				(plus lX w 
				    (times 2.0 layer1X)
				)
			    )
			    else 
			    (setq lX 
				(plus lleftX 
				    (times 
					(difference c 1.0) cutPitchX
				    )
				)
			    ) 
			    (setq uX 
				(plus lX w)
			    )
			)
			(for r 1 numClusterCutsY 
			    (if 
				(equal side "vertical") then 
				(setq lY 
				    (difference 
					(plus llY 
					    (times 
						(difference r 1.0) cutPitchY
					    )
					) layer1Y
				    )
				) 
				(setq uY 
				    (plus lY l 
					(times 2.0 layer1Y)
				    )
				)
				else 
				(setq lY 
				    (plus llY 
					(times 
					    (difference r 1.0) cutPitchY
					)
				    )
				) 
				(setq uY 
				    (plus lY l)
				)
			    )
			    (when 
				(and 
				    (lessp lX uX) 
				    (lessp lY uY)
				) 
				(setq rect 
				    (dbCreateRect tcCellView viaLayer 
					(list 
					    (range lX lY) 
					    (range uX uY)
					)
				    )
				) 
				(dbMoveFig rect tcCellView 
				    (list 
					(range moveX moveY) "R0"
				    )
				) 
				(dbAddFigToNet rect net)
			    ) 
			    (when 
				(and 
				    (nequal viaLayer viaLayer2) 
				    (nequal viaLayer2 "")
				) 
				(case side 
				    ("vertical" 
					(setq x1 
					    (plus lX encByVia)
					) 
					(setq x2 
					    (difference uX encByVia)
					) 
					(when 
					    (and 
						(lessp x1 x2) 
						(lessp lY uY)
					    ) 
					    (setq rect 
						(dbCreateRect tcCellView viaLayer2 
						    (list 
							(range x1 lY) 
							(range x2 uY)
						    )
						)
					    ) 
					    (dbMoveFig rect tcCellView 
						(list 
						    (range moveX moveY) "R0"
						)
					    ) 
					    (dbAddFigToNet rect net)
					)
				    ) 
				    ("horizontal" 
					(setq y1 
					    (plus lY encByVia)
					) 
					(setq y2 
					    (difference uY encByVia)
					) 
					(when 
					    (and 
						(lessp y1 y2) 
						(lessp lX uX)
					    ) 
					    (setq rect 
						(dbCreateRect tcCellView viaLayer2 
						    (list 
							(range lX y1) 
							(range uX y2)
						    )
						)
					    ) 
					    (dbMoveFig rect tcCellView 
						(list 
						    (range moveX moveY) "R0"
						)
					    ) 
					    (dbAddFigToNet rect net)
					)
				    ) 
				    (t 
					(setq x1 
					    (plus lX encByVia)
					) 
					(setq x2 
					    (difference uX encByVia)
					) 
					(when 
					    (lessp x1 x2) 
					    (setq y1 
						(plus lY encByVia)
					    ) 
					    (setq y2 
						(difference uY encByVia)
					    ) 
					    (when 
						(lessp y1 y2) 
						(setq rect 
						    (dbCreateRect tcCellView viaLayer2 
							(list 
							    (range x1 y1) 
							    (range x2 y2)
							)
						    )
						) 
						(dbMoveFig rect tcCellView 
						    (list 
							(range moveX moveY) "R0"
						    )
						) 
						(dbAddFigToNet rect net)
					    )
					)
				    )
				)
			    )
			)
		    )
		)
	    )
	    (unless 
		(or viaAreaPin 
		    (equal viaLayer layer2)
		) 
		(setq dot 
		    (dbCreateDot tcCellView viaLayer 
			(range 0.0 centerY)
		    )
		) 
		(setq pin 
		    (dbCreatePin net dot)
		) 
		(dbSetq pin 
		    (list "right" "left" "top" "bottom") accessDir
		)
	    ) 
	    (unless 
		(equal layer2 "") 
		(setq x1 
		    (difference originX layer2X)
		) 
		(setq x2 
		    (plus urightX layer2X)
		) 
		(when 
		    (lessp x1 x2) 
		    (setq y1 
			(difference originY layer2Y)
		    ) 
		    (setq y2 
			(plus urY layer2Y)
		    ) 
		    (when 
			(lessp y1 y2) 
			(setq rect 
			    (dbCreateRect tcCellView layer2 
				(list 
				    (range x1 y1) 
				    (range x2 y2)
				)
			    )
			) 
			(dbMoveFig rect tcCellView 
			    (list 
				(range moveX moveY) "R0"
			    )
			) 
			(dbAddFigToNet rect net)
			(when layer2AreaPin 
			    (setq pin 
				(dbCreatePin net rect)
			    ) 
			    (dbSetq pin direction2 accessDir)
			) 
			(when 
			    (nequal layer2Implant "") 
			    (setq x1 
				(difference x1 layer2ImpEnc)
			    ) 
			    (setq x2 
				(plus x2 layer2ImpEnc)
			    ) 
			    (when 
				(lessp x1 x2) 
				(setq y1 
				    (difference y1 layer2ImpEnc)
				) 
				(setq y2 
				    (plus y2 layer2ImpEnc)
				) 
				(when 
				    (lessp y1 y2) 
				    (setq rect 
					(dbCreateRect tcCellView layer2Implant 
					    (list 
						(range x1 y1) 
						(range x2 y2)
					    )
					)
				    ) 
				    (dbMoveFig rect tcCellView 
					(list 
					    (range moveX moveY) "R0"
					)
				    ) 
				    (dbAddFigToNet rect net)
				)
			    )
			)
		    )
		)
		(unless layer2AreaPin 
		    (setq dot 
			(dbCreateDot tcCellView layer2 
			    (range 0.0 centerY)
			)
		    ) 
		    (setq pin 
			(dbCreatePin net dot)
		    ) 
		    (dbSetq pin direction2 accessDir)
		) 
		(when 
		    (and 
			(null layer2AreaPin) 
			(equal viaLayer layer2)
		    ) 
		    (if 
			(equal i 1) then 
			(case orientation 
			    (("both" "top") 
				(setq dot 
				    (dbCreateDot tcCellView layer2 
					(range 0.0 
					    (plus centerY l)
					)
				    )
				) 
				(setq pin 
				    (dbCreatePin net dot)
				) 
				(dbSetq pin direction2 accessDir)
			    ) 
			    ("bottom" 
				(setq dot 
				    (dbCreateDot tcCellView layer2 
					(range 0.0 
					    (difference centerY l)
					)
				    )
				) 
				(setq pin 
				    (dbCreatePin net dot)
				) 
				(dbSetq pin direction2 accessDir)
			    ) 
			    (t 
				(setq dot 
				    (dbCreateDot tcCellView layer2 
					(range 0.0 
					    (difference centerY 
						(quotient l 2.0)
					    )
					)
				    )
				) 
				(setq pin 
				    (dbCreatePin net dot)
				) 
				(dbSetq pin direction2 accessDir) 
				(setq dot 
				    (dbCreateDot tcCellView layer2 
					(range 0.0 
					    (plus centerY 
						(quotient l 2.0)
					    )
					)
				    )
				)
				(setq pin 
				    (dbCreatePin net dot)
				) 
				(dbSetq pin direction2 accessDir)
			    )
			) else
			(setq dot 
			    (dbCreateDot tcCellView layer2 
				(range 0.0 
				    (difference centerY l)
				)
			    )
			) 
			(setq pin 
			    (dbCreatePin net dot)
			) 
			(dbSetq pin direction2 accessDir)
		    )
		)
	    ) 
	    (setq Y llY) 
	    (setq llY 
		(minus urY)
	    ) 
	    (setq urY 
		(minus Y)
	    )
	    (setq centerY 
		(minus centerY)
	    )
	) 
	(case side 
	    ("left-leak" 
		(setq x2 
		    (quotient 
			(minus w) 2.0
		    )
		) 
		(setq x1 
		    (difference x2 leakLength)
		) 
		(setq y1 x2) 
		(setq y2 
		    (minus x2)
		)
	    ) 
	    ("right-leak" 
		(setq x1 
		    (quotient w 2.0)
		) 
		(setq x2 
		    (plus x1 leakLength)
		) 
		(setq y1 
		    (minus x1)
		) 
		(setq y2 x1)
	    ) 
	    ("top-leak" 
		(setq x2 
		    (quotient l 2.0)
		) 
		(setq x1 
		    (minus x2)
		) 
		(setq y1 x2) 
		(setq y2 
		    (plus y2 leakLength)
		)
	    )
	    ("bottom-leak" 
		(setq x1 
		    (quotient 
			(minus l) 2.0
		    )
		) 
		(setq x2 
		    (minus x1)
		) 
		(setq y2 x1) 
		(setq y1 
		    (difference y2 leakLength)
		)
	    ) 
	    (t 
		(setq x1 0.0) 
		(setq x2 0.0) 
		(setq y1 0.0) 
		(setq y2 0.0)
	    )
	) 
	(when 
	    (and 
		(lessp x1 x2) 
		(lessp y1 y2)
	    ) 
	    (setq rect 
		(dbCreateRect tcCellView viaLayer 
		    (list 
			(range x1 y1) 
			(range x2 y2)
		    )
		)
	    ) 
	    (dbMoveFig rect tcCellView 
		(list 
		    (range moveX moveY) "R0"
		)
	    ) 
	    (dbAddFigToNet rect net)
	)
    )
)
;
;
tfcDefineDeviceClassProp(
; (viewName        devClassName      propName          propValue)
  (symbolic        syEnhContact      function          "contact")
)


