;extractMOS(deviceName  recLayer gateLayer sdLayer bulkLayer [spiceModel])
extractMOS("nch_na" "ngate_na" "PO" "OD" "substrate")

;extractMOS(deviceName  recLayer gateLayer sdLayer bulkLayer [spiceModel])
