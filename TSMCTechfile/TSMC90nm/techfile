     ( minSpacing                "M9"	
	(( "width"   nil  nil 	 "length"   nil   nil  )	 )
	(
	   (0.0       0.0       )	2.0       
	   (0.0       12.0      )	3.0       
	   (12.0      0.0       )	3.0       
	   (12.0      12.0      )	3.0       
	)
     )
     ( minDensity                "M9"	
