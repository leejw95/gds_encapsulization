     ( minOppExtension            "M6"	"VIA6"		(0.005 0.05) )
     ( minOppExtension            "M5"	"VIA4"		(0.005 0.05) )
     ( minOppExtension            "M6"	"VIA5"		(0.005 0.05) )
     ( minOppExtension            "M7"	"VIA6"		(0.005 0.05) )
     ( minOppExtension            "M7"	"VIA7"		(0.03 0.08) )
     ( minOppExtension            "M8"	"VIA7"		(0.03 0.08) )
     ( minOppExtension            "M8"	"VIA8"		(0.03 0.08) )
     ( minOppExtension            "M9"	"VIA8"		(0.3 0.4) )
    ) ;orderedSpacings

    antennaModels(
