	(
	   0.36      1         
	   1.8       2         
	)
     )
     ( minSpacing                "M8"	
	(( "width"   nil  nil 	 "length"   nil   nil  )	 )
	(
	   (0.0       0.0       )	0.42      
	   (0.0       1.5       )	0.42      
	   (0.0       4.5       )	0.42      
	   (1.5       0.0       )	0.42      
	   (1.5       1.5       )	0.5       
	   (1.5       4.5       )	0.5       
	   (4.5       0.0       )	0.42      
	   (4.5       1.5       )	0.5       
	   (4.5       4.5       )	1.5       
