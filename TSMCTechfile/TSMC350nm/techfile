;********************************
; LAYER RULES
;********************************
layerRules(
 viaLayers(
 ;( layer1      viaLayer      layer2 )
 ;( ------      --------      ------ )
  ( PDIFF         CONT        METAL1 )
  ( NDIFF         CONT        METAL1 )
  ( DIFF          CONT        METAL1 )
  ( POLY1         CONT        METAL1 )
  ( POLY2         CONT        METAL1 )
  ( METAL1        VIA12       METAL2 )
  ( METAL2        VIA23       METAL3 )
  ( METAL3        VIA34       METAL4 )
 )

