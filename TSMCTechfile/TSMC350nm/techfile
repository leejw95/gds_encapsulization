devices(

tcCreateCDSDeviceClass()

;
; no syEnhancement devices
;

;
; no syDepletion devices
;

symContactDevice(
; (name viaLayer viaPurpose layer1 purpose1 layer2 purpose2
;  w l (row column xPitch yPitch xBias yBias) encByLayer1 encByLayer2 legalRegion)

  (M1_POLY1 CONT drawing POLY1 drawing METAL1 drawing
  0.4 0.4 (1 1 0.8 0.8 center center) 0.2 0.15 _NA_)

  (M1_P CONT drawing DIFF drawing (PIMP drawing 0.25) METAL1 drawing 
  0.4 0.4 (1 1 0.8 0.8 center center) 0.25 0.15 _NA_)

  (M1_N CONT drawing DIFF drawing (NIMP drawing 0.25) METAL1 drawing 
  0.4 0.4 (1 1 0.8 0.8 center center) 0.25 0.15 _NA_)
  (M2_M1 VIA12 drawing METAL1 drawing METAL2 drawing
  0.5 0.5 (1 1 0.95 0.95 center center) 0.2 0.15 _NA_)

  (M3_M2 VIA23 drawing METAL2 drawing METAL3 drawing
  0.5 0.5 (1 1 0.95 0.95 center center) 0.2 0.15 _NA_)



    
  (M4_M3 VIA34 drawing METAL3 drawing METAL4 drawing
  0.5 0.5 (1 1 0.95 0.95 center center) 0.2 0.15 _NA_)
)

tfcDefineDeviceProp(
; (viewName	deviceName	propName	propValue)
  (symbolic	M1_POLY1	viaType		"default")
  (symbolic	M1_P		viaType		"default")
  (symbolic	M1_N		viaType		"default")
  (symbolic	M2_M1		viaType		"default")
  (symbolic	M3_M2		viaType		"default")
  (symbolic	M4_M3		viaType		"default")
)

symPinDevice(
; (name maskable layer1 purpose1 w1 layer2 purpose2 w2 legalRegion)
  (POLY1_T t POLY1 drawing 0.35 _NA_ _NA_ _NA_ _NA_)
  (METAL1_T t METAL1 drawing 0.5 _NA_ _NA_ _NA_ _NA_)
  (METAL2_T t METAL2 drawing 0.6 _NA_ _NA_ _NA_ _NA_)
  (METAL3_T t METAL3 drawing 0.6 _NA_ _NA_ _NA_ _NA_)
  (METAL4_T t METAL4 drawing 0.6 _NA_ _NA_ _NA_ _NA_)
)
cdsViaDevice(
 ; (name cutLayer cutPurpose layer1 purpose1 layer2 purpose2
 ;  row column origin stackedVias cutLayerW cutLayerL xCutSpacing yCutSpacing
 ;  layer1XEnc layer1YEnc layer2XEnc layer2YEnc layer1Dir layer2Dir
 ;  layer1XDirOverride layer1YDirOverride layer2XDirOverride layer2YDirOverride
 ;  l_implantLayer1 l_implantLayer2 diffSpacing abutClass)

        (M1_POLYc CONT drawing POLY1 drawing METAL1 drawing
        1 1 centerCenter t 0.4 0.4 0.4 0.4
        0.2 0.2 0.15 0.15 "XY" "XY"
        0.2 0.2 0.15 0.15
        nil nil _NA_ "")
        (M1_Pc CONT drawing DIFF drawing METAL1 drawing
        1 1 centerCenter t 0.4 0.4 0.4 0.4
        0.25 0.25 0.15 0.15 "XY" "XY"
        0.25 0.25 0.15 0.15
        ( ((PIMP drawing) 0.25)) nil _NA_ "")

        (M1_Nc CONT drawing DIFF drawing METAL1 drawing
        1 1 centerCenter t 0.4 0.4 0.4 0.4
        0.25 0.25 0.15 0.15 "XY" "XY"
        0.25 0.25 0.15 0.15
        ( ((NIMP drawing) 0.25)) nil _NA_ "")
        (M2_M1c VIA12 drawing METAL1 drawing METAL2 drawing
        1 1 centerCenter t 0.5 0.5  0.45 0.45
        0.2 0.2 0.15 0.15 "Y" "X"
        0.2 0.2 0.15 0.15  nil  nil  _NA_ ""   )

        (M3_M2c VIA23 drawing METAL2 drawing METAL3 drawing
        1 1 centerCenter t 0.5 0.5  0.45 0.45
        0.2 0.2 0.15 0.15 "Y" "X"
        0.2 0.2 0.15 0.15  nil  nil  _NA_ ""   )



        (M4_M3c VIA34 drawing METAL3 drawing METAL4 drawing
        1 1 centerCenter t 0.5 0.5  0.45 0.45
        0.2 0.2 0.15 0.15 "Y" "X"
        0.2 0.2 0.15 0.15  nil  nil  _NA_ ""   )
)
;
; no ruleContact devices
;

