  ( minSpacingRange	"METAL7" ("0.240 RANGE	0.390 10.000" "0.600 RANGE 10.050 100000.000")	)
  ( offset		"METAL8"	0.69	)
  ( minSpacingRange	"METAL8" ("0.600 RANGE	10.050 100000.000")	)
  ( sameNet		"METAL1"	"METAL1"	0.18	)
