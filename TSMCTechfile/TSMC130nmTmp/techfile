  (symbolic        via3ts            res               1.020000)
  (symbolic        via3ts            topstack          t)
)

tcDeclareDevice( "symbolic" "syEnhContact" "via4"
    ( (viaLayer "VIA45") (viaLayer2 "") (encByVia 0.000000)
      (layer1 "METAL4") (layer1Implant "") (layer1ImpEnc 0.000000)
      (layer2 "METAL5") (layer2Implant "") (layer2ImpEnc 0.000000)
      (encByLayer1 0.060000) (encByLayer2 0.005000) (layer1XEnc 0.005000)
      (layer1YEnc 0.000000) (layer2XEnc 0.060000) (layer2YEnc 0.000000) )
    ( (w 0.190000) (l 0.190000) (row 1)
      (column 1) (xPitch 1.000000) (yPitch 1.000000)
      (offset 0.000000) (side "center") (xBias "center")
      (yBias "center") (layer1Dir "xy") (layer2Dir "xy")
      (leakLength 0.000000) (areaPin "layer1") )
)
;

tfcDefineDeviceProp(
; (viewName        deviceName        propName          propValue)
  (symbolic        via4              res               1.020000)
)

tcDeclareDevice( "symbolic" "syEnhContact" "via4ts"
    ( (viaLayer "VIA45") (viaLayer2 "") (encByVia 0.000000)
      (layer1 "METAL4") (layer1Implant "") (layer1ImpEnc 0.000000)
      (layer2 "METAL5") (layer2Implant "") (layer2ImpEnc 0.000000)
      (encByLayer1 0.205000) (encByLayer2 0.005000) (layer1XEnc 0.025000)
      (layer1YEnc 0.000000) (layer2XEnc 0.060000) (layer2YEnc 0.000000) )
    ( (w 0.190000) (l 0.190000) (row 1)
      (column 1) (xPitch 1.000000) (yPitch 1.000000)
      (offset 0.000000) (side "center") (xBias "center")
      (yBias "center") (layer1Dir "xy") (layer2Dir "xy")
      (leakLength 0.000000) (areaPin "layer1") )
)
;

tfcDefineDeviceProp(
; (viewName        deviceName        propName          propValue)
  (symbolic        via4ts            res               1.020000)
  (symbolic        via4ts            topstack          t)
)

tcDeclareDevice( "symbolic" "syEnhContact" "via5"
    ( (viaLayer "VIA56") (viaLayer2 "") (encByVia 0.000000)
